{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@fakes/*": [
        "./src/fakes/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}